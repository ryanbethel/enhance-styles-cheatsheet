{
  "name": "@enhance/arc-plugin-enhance-styles-cheatsheet",
  "version": "0.0.3",
  "scripts": {
    "lint": "eslint ./**/*.mjs --fix",
    "start": "begin dev",
    "test": "run-s lint test:**",
    "test:unit": "tape test/**/*.*js | tap-arc",
    "hydrate": "rm -rf test/mock-app/src/plugins/cheatsheet && mkdir test/mock-app/src/plugins/cheatsheet && cp -r src test/mock-app/src/plugins/cheatsheet/ && cp ./index.js test/mock-app/src/plugins/cheatsheet",
    "demo": "npm run hydrate && cd ./test/mock-app/ && begin dev"
  },
  "dependencies": {
    "@enhance/arc-plugin-enhance": "^6.1.2",
    "css-tree": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "npm-run-all": "^4.1.5",
    "tap-arc": "^0.3.5",
    "tape": "^5.6.3"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        2
      ]
    },
    "ignorePatterns": [],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2022
    }
  }
}
